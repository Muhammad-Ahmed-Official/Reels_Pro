{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "outDir": "./dist",
    "noEmit": true,
    "resolveJsonModule": true
  },
    "include": [
  "./**/*.ts",
  "../Models/Chat.model.ts",
  "../Models/Notification.model.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}